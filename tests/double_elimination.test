<?php
class DoubleElimimationTestCase extends TourneyWebTestCase {
  protected $controller = 'DoubleEliminationController';
  
  /**
   * Test info.
   */
  public static function getInfo() {
    return array(
      'name' => t('Double Elimination Tournament'),
      'description' => t('Steps through setting up a double elimination tournament and playing all the way through.'),
      'group' => t('Tourney'),
    );
  }
  

    /**
     * Testing 4 contestants
     */
    public function testDoubleElim4() {
      // Create the tournament.
      $tournament_name = $this->createTourney($this->controller, 4);

	  // Upper Bracket
      // Set contestants of the First Match as same user.
      $edit = array();
      // Match 1 Slot 1
      $edit['contestant1'] = 'user.3';
      // Match 1 Slot 2
      $edit['contestant2'] = 'user.3';
      // Save Match 1
      $this->drupalPost('tourney/match/1/edit', $edit, t('Save'));
      // Confirm Tourney Module can't set the same user twice.
      $this->assertRaw(t('Contestant 1 and Contestant 2 cannot be the same person'));

      // Now change contestant 2 to a different user.
      // Match 1 Slot 2.
      $edit['contestant2'] = 'user.6';
      // Save Match 1 (/tourney/match/1)
      $this->drupalPost('tourney/match/1/edit', $edit, t('Save'));
      // Confirm Match has been saved.
      $this->assertRaw(t('Your match has been saved'));
      // Make sure the link to Tourament is present.
      $this->assertLink(t('Go to tournament !tournament', array('!tournament' => $tournament_name)));

      // Play First Match
      $edit = array();
      $this->drupalPost('tourney/match/1', $edit, t('Win'), array(), array(), 'tourney-game-win-form');
      // Check to see if winner was successfully set passing in "Players moved to next round" text.
      $this->assertRaw(t('Players moved to next round'));
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.3</div>');

      // Match 2
      $edit = array();
      // Match 2 Slot 1
      $edit['contestant1'] = 'user.4';
      // Match 2 Slot 2
      $edit['contestant2'] = 'user.5';
      // Save Match 2 (/tourney/match/2)
      $this->drupalPost('tourney/match/2/edit', $edit, t('Save'));
      // Make sure Match has been saved.
      $this->assertRaw(t('Your match has been saved'));

      // Play Second Match
      $edit = array();
      $this->drupalPost('tourney/match/2', $edit, t('Win'), array(), array(), 'tourney-game-win-form--2');
      // Check to see if winner was successfully set, passing in "Players moved to next round" text.
      $this->assertRaw(t('Players moved to next round'));
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.5</div>');

      // Play Third Match
      $edit = array();
      $this->drupalPost('tourney/match/3', $edit, t('Win'), array(), array(), 'tourney-game-win-form');
      // Check to see if winner users of previous matches are being passed into this game correctly
      $this->assertRaw('<tr class="odd"><td>user.3</td>');
      $this->assertRaw('<tr class="even"><td>user.5</td>');
      // Check to see if winner was successfully set, passing in "Players moved to next round" text.
      $this->assertRaw(t('Players moved to next round'));
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.3</div>');
      
      // Loser Bracket
      // Match 4
      // Check to see if winner of previous match is being passed into this match correctly 
      $this->drupalGet('tourney/match/4');
      $this->assertRaw(t('<tr class="odd"><td>user.4</td>'));
      $this->assertRaw(t('<tr class="even"><td>user.6</td>'));

      // Play Forth Match
      $edit = array();
      $this->drupalPost('tourney/match/4', $edit, t('Win'), array(), array(), 'tourney-game-win-form--2');
      // Check to see if winner was successfully set, passing in "Players moved to next round" text.
      $this->assertRaw(t('Players moved to next round'));
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.6</div>');

      // Match 5
      // Check to see if winner of previous match is being passed into this match correctly 
      $this->drupalGet('tourney/match/5');
      $this->assertRaw(t('<tr class="odd"><td>user.5</td>'));
      $this->assertRaw(t('<tr class="even"><td>user.6</td>'));

      // Play Fifth Match
      $edit = array();
      $this->drupalPost('tourney/match/5', $edit, t('Win'), array(), array(), 'tourney-game-win-form');
      // Check to see if winner was successfully set, passing in "Players moved to next round" text.
      $this->assertRaw(t('Players moved to next round'));
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.5</div>');
      
	  // Tourament Championship Matches
      // Match 6
      // Check to see if winner of previous match is being passed into this match correctly 
      $this->drupalGet('tourney/match/6');
      $this->assertRaw(t('<tr class="odd"><td>user.3</td>'));
      $this->assertRaw(t('<tr class="even"><td>user.5</td>'));

	  // Set number of games played in championship match to 3
	  $edit = array();
      $edit ['games'] = '3';
      $this->drupalPost('tourney/match/6/edit', $edit, t('Save'));

      // Play Sixth Match
      $edit = array();
      // Set Game 1 Winner
      $this->drupalPost('tourney/match/6', $edit, t('Win'), array(), array(), 'tourney-game-win-form');
      // Set Game 2 Winner
      $this->drupalPost('tourney/match/6', $edit, t('Win'), array(), array(), 'tourney-game-win-form');
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.3</div>');
   

      // Checking 4 man Tourney Winner is displaying on the Tournament page 
      $edit = array();
      $this->drupalGet('tourney/tournament/1');
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.3</div>');

	  /** 
	   * Testing that the tie-breaker is getting set and played properly, by editing championship match winner to be from bottom bracket
	   */
	  // Edit championship match, game 2 to set lower bracket as winner
	  $edit = array('game.8' => 'user.5');
	  $this->drupalPost('tourney/match/6/edit', $edit, t('Save'));
	
	  // Play 3rd game making slot 2 win match.
	  $edit = array();
	  $this->drupalPost('tourney/match/6', $edit, t('Win'), array(), array(), 'tourney-game-win-form--2');
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.5</div>');
	
	  // Match 7 - Tie Breaker
      // Check to see if winner of previous match is being passed into this match correctly 
      $this->drupalGet('tourney/match/7');
      $this->assertRaw(t('<tr class="odd"><td>user.5</td>'));
      $this->assertRaw(t('<tr class="even"><td>user.3</td>'));

	  // Play Seventh Match
      $edit = array();
      $this->drupalPost('tourney/match/7', $edit, t('Win'), array(), array(), 'tourney-game-win-form');
      // Check to see if the winner is printed on the page.
      $this->assertRaw(t('user.5'));

	  // Checking 4 man Tourney Winner is displaying on the Tournament page 
      $edit = array();
      $this->drupalGet('tourney/tournament/1');
      // Check to see if the winner is printed on the page.
      $this->assertRaw('<div class="field-label">Winner:</div><div class="field-content">user.5</div>');

      // End of 4 Man Tourney
      $this->pass("End of 4 Man Double Elimination Tournament ");
    }

